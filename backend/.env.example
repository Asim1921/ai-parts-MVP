PORT=4000
MONGODB_URI=mongodb://localhost:27017/parts-platform
FRONTEND_URL=http://localhost:3000
UPLOAD_DIR=./uploads

# OpenAI Vision API (optional). When set, scan images are analyzed with GPT-4o to identify parts against the TerreMax catalog.
OPENAI_API_KEY=
OPENAI_VISION_MODEL=gpt-4o

# Optional: Python vision service URL. If set and OPENAI_API_KEY is not set, backend calls this for analysis.
VISION_SERVICE_URL=http://localhost:5000
